<?xml version="1.0" ?>
<robot name="test" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <link name="base_link">
  </link>
  
  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="chassis"/>
  </joint>

  <link name="chassis">
    
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="0.0" iyz="0.0" ixz="0.0"/>
    </inertial>
    
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://basic_robot/meshes/chassis.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://basic_robot/meshes/chassis.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="left_wheel_1">
    
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="-0.0" iyz="-0.0" ixz="0.0"/>
    </inertial>
    
    <visual>
      <origin xyz="0.5 -0.0 -0.25" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://basic_robot/meshes/left_wheel_1.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="0.5 -0.0 -0.25" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://basic_robot/meshes/left_wheel_1.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <link name="right_wheel_1">
    
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0"/>
      <inertia ixx="0.0" iyy="0.0" izz="0.0" ixy="-0.0" iyz="-0.0" ixz="0.0"/>
    </inertial>
    
    <visual>
      <origin xyz="-0.5 -0.0 -0.25" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://basic_robot/meshes/right_wheel_1.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </visual>
    <collision>
      <origin xyz="-0.5 -0.0 -0.25" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://basic_robot/meshes/right_wheel_1.stl" scale="0.1 0.1 0.1"/>
      </geometry>
    </collision>
  </link>

  <joint name="Revolute 1" type="continuous">
    <origin xyz="-0.5 0.0 0.25" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="left_wheel_1"/>
    <axis xyz="-1.0 0.0 0.0"/>
  </joint>

  <joint name="Revolute 2" type="continuous">
    <origin xyz="0.5 0.0 0.25" rpy="0 0 0"/>
    <parent link="base_link"/>
    <child link="right_wheel_1"/>
    <axis xyz="1.0 0.0 0.0"/>
  </joint>

</robot>
